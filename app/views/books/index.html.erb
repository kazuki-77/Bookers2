<%= @books.each do |book| %>
<div>
  <%= link_to book_path(book.id) do %>
  <!--%= attachment_image_tag book.user, :profile_image, :fill, 60, 60, fallback: "no-image.jpg" %> -->
  <td>投稿ユーザー画像:<%= image_tag('sample-author1.ipg') %></td>
  <td><%= book.name %></td>
  <!--% end %-->
  <td><%= book.title %></td>
  <td><%= book.body %></td>
  <td><%= link_to "Edit",edit_book_path(book) %></td>
  <td><%= link_to "Destroy", book_path(book), method: :delite, "deta-confirm" => "本当に消しますか？" %></td>
</div>
<% end %>